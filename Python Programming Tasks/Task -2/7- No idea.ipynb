    "A = set(map(int, raw_input().split()))\n",
    "B = set(map(int, raw_input().split()))\n",
    "\n",
    "happiness = 0\n",
    "for i in n:\n",
    "    if i in A:\n",
    "        happiness += 1\n",
    "    elif i in B:\n",
    "        happiness -= 1\n",
    "\n",
    "print happiness"
