   "source": [
    "import math\n",
    "ab=int(input())\n",
    "bc=int(input())\n",
    "h=math.hypot(ab,bc)\n",
    "h1=h/2\n",
    "bca=math.asin(1*ab/h)\n",
    "bm=math.sqrt((bc**2+h1**2)-(2*bc*h1*math.cos(bca)))\n",
    "mbc=math.asin(math.sin(bca)*h1/bm)\n",
    "print(int(round(math.degrees(mbc),0)),'\\u00B0',sep='')"
   ]
  }
