   "source": [
    "def mutate_string(string, position, character):\n",
    "    lis = list(string)\n",
    "    lis[position]=character\n",
    "    return ''.join(lis)\n",
    " \n",
    "if __name__ == '__main__':\n",
    "    s = input()\n",
    "    i, c = input().split()\n",
    "    s_new = mutate_string(s, int(i), c)\n",
    "    print(s_new)"
   ]
  }
